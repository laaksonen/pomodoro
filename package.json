{
  "name": "react-redux-simple-starter",
  "version": "1.0.0",
  "description": "A simple starter boilerplate for a webapp using react and redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/laaksonen/react-redux-simple-starter"
  },
  "keywords": [
    "react",
    "redux",
    "starter",
    "boilerplate"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon bin/server",
    "dev": "NODE_ENV=development nodemon bin/server",
    "prod": "NODE_ENV=production node bin/server",
    "clean": "rimraf dist",
    "lint": "eslint src test server config",
    "test": "mocha --recursive --compilers js:babel-register --require ./test/setup.js",
    "test:dev": "npm run test -- --watch",
    "compile": "NODE_ENV=production webpack --stats --config config/webpack.config.js --progress",
    "deploy": "NODE_ENV=production npm run lint && npm run test && npm run clean && npm run compile",
    "stats": "NODE_ENV=production webpack --config config/webpack.config.js --profile --json > stats.json"
  },
  "author": "Lauri Laaksonen <lauri@laaksonen.io> (http://laaksonen.io)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.17.0",
    "lodash": "^4.9.0",
    "node-sass": "^3.7.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "chai": "^3.5.0",
    "eslint": "^2.6.0",
    "eslint-plugin-react": "^5.1.1",
    "jsdom": "^8.4.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
